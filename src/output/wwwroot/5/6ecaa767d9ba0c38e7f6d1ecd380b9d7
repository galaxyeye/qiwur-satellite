	        for(var n = 0, m = _temparr.length; n < m; n++){
	        	var tmp = _temparr[n].split(':');
	        	if(tmp.length > 1 && tmp[0] == 55){
	        		itemInfo.brand_id = tmp[1];
	        		continue;
	        	}
	        }
        }
      }
    }
  }  
  itemInfo.bt_state = ~~'5';
  var _desc = $('#sea_desc');
  var _text = _desc.text();
  if(_text){    
    _text = _text.replace(/(http:\/\/)?(((url\.cn)|(u\.yixun\.com)|(event\.yixun\.com))[\/\w\.-]+)/gi, '<a href="http://$2" target="_blank">$1$2</a>');
    $('.xdesc').html(_text);
  }
	// 类目那里的url
	if(itemInfo.nca_list.length){
		//大类页逻辑
        if(itemInfo.new_nca){
	        var strNewNcaUrl = "";
        	var new_nca = $.parseJSON(itemInfo.new_nca);
        	var strWhid = itemInfo.whid;
        	if(new_nca[itemInfo.whid] && new_nca[itemInfo.whid]["status"] == 1){
        		strNewNcaUrl = new_nca[strWhid]["url"];
        	}
        
	        var strCateUrl = "http://searchex.yixun.com/html?path=";
	        for ( var dwLoop = 0; dwLoop < itemInfo.nca_list.length ; dwLoop++ ){
	        	var category_url = "";
	        	var nca_ytag = "";
                var nca_id = itemInfo["nca_list"][dwLoop]["id"];
	            if( dwLoop == 0 ){
	                strCateUrl += nca_id;
	                if ( strNewNcaUrl.length == 0 ){
	                	category_url = strCateUrl + "&area=" + strWhid;
	                	nca_ytag = '2000'+(dwLoop + 2);
	                } else {
	                    category_url = strNewNcaUrl;
	                	nca_ytag = '20009';
	                }
	            }else{
	                strCateUrl += "t" + itemInfo["nca_list"][dwLoop]["id"];
	                category_url = strCateUrl + "&area=" + strWhid;
                	nca_ytag = '2000'+(dwLoop + 2);
	            }
	            $(".mod_crumb a[nca-id='"+nca_id+"']").attr("href", category_url).attr("ytag", nca_ytag);
	        }
        }
        
	}
    var tmp_nca_list = itemInfo.nca_list;
	if(tmp_nca_list.length > 0){
		var crum_id = [];
		for(var i = 0;i < tmp_nca_list.length;i++){
			crum_id.push(tmp_nca_list[i].id);
		}
		$(".mod_crumb a[ytag='20020']").attr("href", "http://searchex.yixun.com/"+crum_id.join("t")+"-"+itemInfo.whid+"-/55e"+itemInfo.brand_id);
	}
	
})();
